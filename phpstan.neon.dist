includes:
    - phpstan-baseline.neon

parameters:
    level: 6
    paths:
        - src
        - database
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    treatPhpDocTypesAsCertain: false
    universalObjectCratesClasses:
        - Aldeebhasan\LaravelEventTracker\Models
